{
  "name": "eve-equinox-api",
  "version": "0.0.1",
  "description": "eve-equinox-api server",
  "main": "dist/server.js",
  "source": "src/server.ts",
  "targets": {
    "main": {
      "distDir": "./dist",
      "context": "node"
    }
  },
  "scripts": {
    "start:prod": "npm run build && node dist/server.js",
    "start:dev": "npx nodemon",
    "start": "npm run start:dev",
    "build": "npm rebuild && parcel build src/server.ts",
    "lint": "eslint . --ext .ts",
    "format": "eslint . --ext .ts --fix",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-watch": "run-script-os",
    "prettier-watch:win32": "onchange \"src/**/*.ts\" -- prettier --write {{changed}}",
    "prettier-watch:darwin:linux": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "prettier-watch:default": "onchange 'src/**/*.ts' -- prettier --write {{changed}}",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --verbose -w 5 --coverage",
    "prepare": "npx husky install",
    "bundle-analyzer": "parcel build src/server.ts --reporter @parcel/reporter-bundle-analyzer",
    "start:redis": "docker-compose up -d redis",
    "migrate": "drizzle-kit up:pg",
    "generate": "drizzle-kit generate:pg"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run prettier-format && npm run lint"
    }
  },
  "engines": {
    "node": ">= 12"
  },
  "private": true,
  "devDependencies": {
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/passport": "^1.0.12",
    "@types/passport-oauth2": "^1.4.12",
    "@types/passport-oauth2-refresh": "^1.1.1",
    "@types/pg": "^8.6.6",
    "@types/sequelize": "^4.28.14",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "buffer": "^5.5.0",
    "drizzle-kit": "^0.17.6",
    "eslint": "^8.37.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "events": "^3.1.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "parcel": "^2.8.3",
    "prettier": "^2.8.7",
    "process": "^0.11.10",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "sequelize-cli": "^6.6.0",
    "sequelize-typescript-cli": "^1.1.0",
    "stream-http": "^3.1.0",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "url": "^0.11.0"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "connect-redis": "^7.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "drizzle-orm": "^0.23.4",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.1",
    "ioredis": "^5.3.1",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-http-header-strategy": "^1.1.0",
    "passport-oauth2": "^1.7.0",
    "passport-oauth2-refresh": "^2.1.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^19.8.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.31.0",
    "sequelize-typescript": "^2.1.5"
  }
}